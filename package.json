{
  "name": "algo-vis",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently yarn:dev:*",
    "build": "yarn build:re && yarn build:js",
    "clean": "yarn clean:js && yarn clean:re",
    "dev:js": "parcel index.html",
    "build:js": "yarn clean:js && parcel build index.html",
    "clean:js": "rimraf .cache dist",
    "dev:re": "bsb -make-world -w",
    "build:re": "bsb -make-world",
    "clean:re": "bsb -clean-world"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "author": "Nimo Beeren",
  "license": "MIT",
  "dependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "reason-react": ">=0.7.1"
  },
  "devDependencies": {
    "bs-platform": "^8.2.0",
    "concurrently": "^5.3.0",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2"
  }
}
